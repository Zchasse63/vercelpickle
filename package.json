{
  "name": "pickle-b2b-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "echo \"⚠️ Warning: Running without Convex server. Use 'npm run dev:full' instead for full functionality.\" && next dev",
    "dev:full": "concurrently \"convex dev\" \"next dev\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:convex": "vitest --config vitest.convex.config.mts",
    "test:convex:run": "vitest run --config vitest.convex.config.mts",
    "test:convex:coverage": "vitest run --coverage --config vitest.convex.config.mts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:guest": "cypress run --spec \"cypress/e2e/guest-user.cy.ts\"",
    "test:e2e:buyer": "cypress run --spec \"cypress/e2e/buyer-user.cy.ts\"",
    "test:e2e:seller": "cypress run --spec \"cypress/e2e/seller-user.cy.ts\"",
    "test:e2e:admin": "cypress run --spec \"cypress/e2e/admin-user.cy.ts\"",
    "test:visual": "cypress run --spec \"cypress/e2e/visual-testing.cy.ts\"",
    "test:visual:open": "cypress open --e2e --spec \"cypress/e2e/visual-testing.cy.ts\"",
    "test:visual:basic": "cypress run --spec \"cypress/e2e/applitools-basic.cy.ts\"",
    "test:visual:basic:open": "cypress open --e2e --spec \"cypress/e2e/applitools-basic.cy.ts\"",
    "test:visual:working": "cypress run --spec \"cypress/e2e/applitools-working.cy.ts\"",
    "test:visual:working:open": "cypress open --e2e --spec \"cypress/e2e/applitools-working.cy.ts\"",
    "test:visual:checkout": "cypress run --spec \"cypress/e2e/visual-checkout-flow.cy.ts\"",
    "test:visual:checkout:open": "cypress open --e2e --spec \"cypress/e2e/visual-checkout-flow.cy.ts\"",
    "test:visual:profile": "cypress run --spec \"cypress/e2e/visual-user-profile.cy.ts\"",
    "test:visual:profile:open": "cypress open --e2e --spec \"cypress/e2e/visual-user-profile.cy.ts\"",
    "test:visual:admin": "cypress run --spec \"cypress/e2e/visual-admin-dashboard.cy.ts\"",
    "test:visual:admin:open": "cypress open --e2e --spec \"cypress/e2e/visual-admin-dashboard.cy.ts\"",
    "test:visual:seller": "cypress run --spec \"cypress/e2e/visual-seller-dashboard.cy.ts\"",
    "test:visual:seller:open": "cypress open --e2e --spec \"cypress/e2e/visual-seller-dashboard.cy.ts\"",
    "test:visual:integrated": "cypress run --spec \"cypress/e2e/integrated-visual-functional.cy.ts\"",
    "test:visual:integrated:open": "cypress open --e2e --spec \"cypress/e2e/integrated-visual-functional.cy.ts\"",
    "test:visual:all": "cypress run --spec \"cypress/e2e/visual-*.cy.ts,cypress/e2e/integrated-visual-*.cy.ts\"",
    "test:visual:all:open": "cypress open --e2e --spec \"cypress/e2e/visual-*.cy.ts,cypress/e2e/integrated-visual-*.cy.ts\"",
    "test:visual:robust": "cypress run --spec \"cypress/e2e/applitools-robust.cy.ts\"",
    "test:visual:robust:open": "cypress open --e2e --spec \"cypress/e2e/applitools-robust.cy.ts\"",
    "test:all": "./scripts/run-tests.sh --all",
    "test:visual:script": "./scripts/run-visual-tests.sh",
    "test:visual:basic:script": "./scripts/run-basic-visual-test.sh",
    "test:visual:working:script": "./scripts/run-working-visual-tests.sh",
    "test:visual:robust:script": "./scripts/run-robust-visual-test.sh",
    "test:visual:checkout:script": "./scripts/run-checkout-visual-tests.sh",
    "test:visual:profile:script": "./scripts/run-profile-visual-tests.sh",
    "test:visual:seller:script": "./scripts/run-seller-visual-tests.sh",
    "test:visual:admin:script": "./scripts/run-admin-visual-tests.sh",
    "test:visual:expanded:script": "./scripts/run-expanded-visual-tests.sh",
    "test:visual:all:script": "./scripts/run-all-visual-tests.sh",
    "convex": "convex dev",
    "convex:setup": "./scripts/setup-convex.sh",
    "convex:deploy": "convex deploy",
    "convex:seed": "node ./scripts/seed-convex.js",
    "update:timestamps": "./scripts/update-timestamps.sh",
    "checkpoint": "./scripts/create-checkpoint.sh",
    "refactor:status": "node scripts/update-refactoring-status.js",
    "refactor:dashboard": "node scripts/update-dashboard.js",
    "refactor:view": "open dashboard/index.html",
    "refactor:update": "npm run refactor:status && npm run refactor:dashboard",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "latest",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
